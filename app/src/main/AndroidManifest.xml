<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clubededepiadas">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ScreenSlidePagerActivity"></activity>
        <activity android:name=".ShowUsersActivity" />
        <activity android:name=".ListarUserActivity" />
        <activity android:name=".SettingsUserActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CadastroActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>